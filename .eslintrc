{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id",
          "_doc"
        ]
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "password",
        "argsIgnorePattern": "next"
      }
    ]
  },
  "noInlineConfig": true
}
